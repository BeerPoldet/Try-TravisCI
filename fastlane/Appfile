import("./Configfile")

apple_id(configs[:apple_id])

itc_team_id(configs[:apple_id]) # App Store Connect Team ID
team_id(configs[:apple_id]) # Developer Portal Team ID

for_platform :ios do
  for_lane :beta do
    app_identifier(configs[:beta][:app_identifier])
  end

  for_lane :prod do
    app_identifier(configs[:release][:app_identifier])
  end

  for_lane :test do
    app_identifier(configs[:debug][:app_identifier])
  end
end